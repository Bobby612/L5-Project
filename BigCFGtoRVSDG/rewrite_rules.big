react rg_lone_region_return = 
    Node.(
        NodeType.Lambda |
        Import.id |
        Body.Region(0).Ct.CtNode.(
            CtTopNode |
            CtBlock(0).Block.(
                Import.id |
                Body.Region(0).id |
                Export.(Dedge{x}.Loc{y} | id)
            )
        ) |
        Export.(/z Dedge{z}.Loc{y}) |
