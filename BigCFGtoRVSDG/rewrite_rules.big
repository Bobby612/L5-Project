    CtBlock(0).Block.(
        Import.(Dedge{m}.Loc{a} | id) |
        id
    ) |
    Node.(
        Read.(Dedge{m}.Loc{a} | id ) |
        id
    )
    -->
    CtBlock(0).Block.(
        Import.(Dedge{m}.Loc{a} | id) |
        id
    ) | /b
    Node.(
        Read.(Dedge{m}.Loc{b} | id ) |
        New{b}.Old{a} |
        id
    )
    ;

react rg_disconnect_loc_new_node_start_export = 
